    "format-code": "prettier-eslint --write \"src/**/*.js\" \"src/**/*.vue\" \"*.config.js\" \".eslintrc.js\"",
    "test:unit": "vue-cli-service test:unit",
    "test:e2e": "vue-cli-service test:e2e"
  },
  "dependencies": {
    "vue": "^2.5.17",
    "vue-router": "^3.0.1",
    "vuex": "^3.0.1"
