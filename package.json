{
  "name": "my-skeleton",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build",
    "build:skeleton": "cross-env WEBPACK_TARGET=skeleton vue-cli-service build && node skeleton.js",
    "lint": "vue-cli-service lint",
